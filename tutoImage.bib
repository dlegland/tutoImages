@UNPUBLISHED{Emily2006,
  author = {Emily, Mathieu and Franc{\c{o}}is, Olivier},
  title = {A continuous stochastic model for cell sorting},
  note = {arXiv:q-bio.TO/0605035},
  year = {2006},
  abstract = { The differential Adhesion Hypothesis (DAH) is a theory of the organization
	of
	
	cells within a tissue. In this study we introduce a stochastic model
	supporting
	
	the DAH, that can be seen as a continuous version of a discrete model
	of
	
	Graner and Glazier. Our approach is based on the mathematical framework
	
	of Gibbsian marked point processes. We provide a Markov chain Monte
	Carlo
	
	algorithm that can reproduce classical biological patterns, and we
	propose an
	
	estimation procedure for a parameter that quantifies the strength
	of adhesion
	
	between cells. This procedure is tested through simulations.},
